{
  "name": "mcil",
  "displayName": "mcil",
  "description": "Syntax highlighter for the Model Checking Intermediate Language",
  "license": "MIT",
  "version": "0.0.1",
  "preview": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ModelChecker/vscode_syntax_highlighter"
  },
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mcil",
        "aliases": [
          "mcil",
          "mcil"
        ],
        "extensions": [
          "mcil"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mcil",
        "scopeName": "source.mcil",
        "path": "./syntaxes/mcil.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^2.21.1",
    "esbuild": "^0.17.3",
    "js-yaml": "^4.1.0"
  }
}
